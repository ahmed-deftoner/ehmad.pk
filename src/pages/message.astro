---
import Table from "../components/Table.astro";
import Layout from "../layouts/Layout.astro";
type Msg = {
    id: string,
    name: string,
    email: string,
    message: string
}
const response = await fetch('https://idxmkziy70.execute-api.ap-south-1.amazonaws.com/Stage/msg');
const data: Array<Msg> = await response.json();
---
<!-- Data fetched at build can be rendered in HTML -->

<Layout title="Messages">
    <Table data={data}/>
   
</Layout>
